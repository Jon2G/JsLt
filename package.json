{
  "name": "jslt",
  "version": "1.0.0",
  "description": "Desarrollo de una plataforma para la ejecución de pruebas de carga a partir de la definición de un script con JavaScript",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "ts-node ./aws/cdktf/run-bundle.ts",
    "synth": "export NODE_OPTIONS='--max_old_space_size=20384' && cd ./aws/cdktf && cdktf synth",
    "deploy": "export NODE_OPTIONS='--max_old_space_size=20384' && cd ./aws/cdktf && cdktf deploy --auto-approve",
    "bundle-deploy": "npm run bundle && npm run deploy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.19.11",
    "guid-typescript": "^1.0.9",
    "mongodb": "^6.3.0",
    "otplib": "^12.0.1",
    "shared": "file:./shared",
    "ts-node": "^10.9.2",
    "typesafe": "file:./types"
  }
}
